---
interface Props {
    navigation: INavigation
}
const {navigation} = Astro.props;

const staticNavigation: INavigation = {
    identifier: 'static-navigation',
    items: [
        {to: "/", label: "Home"},
    ]
}

const navItems = navigation.items ? navigation.items : staticNavigation.items;
---
<nav id="primary-nav" class="mobile-hidden">
    <ul>
        {navItems.map(item => {
            return (
                    <li>
                        <a href="#home">Home</a>
                    </li>
            );
        })}

        <li><a href="#about">About</a></li>
        <li><a href="#services">Services</a></li>
        <li><a href="#contact">Contact</a></li>
    </ul>
</nav>